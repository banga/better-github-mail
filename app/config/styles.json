{
  "commit": {
    "background": "#e6f1f6",
    "border": "solid 1px #c5d5dd",
    "border-radius": "3px",
    "container": {
      "margin": "1em 0"
    },
    "title": {
      "padding": "10px",
      "margin": "0",
      "z-index": "2"
    },
    "message": {
      "margin": "0",
      "padding": "0 10px 10px",
      "white-space": "pre-wrap"
    },
    "footer": {
      "border-top": "solid 1px #d8e6ec",
      "margin": "0 10px",
      "padding": "10px 0",
      "date": {
